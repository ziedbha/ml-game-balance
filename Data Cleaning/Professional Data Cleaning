al['Year'] == 2017) | (professional['Year'] == 2018))]\n","\n","\n","professional.columns = ['blue_champ1' ,'blue_champ2','blue_champ3', 'blue_champ4', 'blue_champ5', 'red_champ1', 'red_champ2', 'red_champ3', 'red_champ4', \n","                        'red_champ5', 'blue_top', 'red_top', 'blue_jungle', 'red_jungle', 'blue_middle', 'red_middle', 'blue_adc', 'red_adc', 'blue_support', 'red_support', 'blue_player1', 'blue_player2', 'blue_player3', 'blue_player4', 'blue_player5',\n","                        'red_player1', 'red_player2', 'red_player3', 'red_player4', 'red_player5', 'blue_win', 'Season', 'Year']\n","\n","columns_to_convert=['blue_champ1' ,'blue_champ2','blue_champ3', 'blue_champ4', 'blue_champ5',  'red_champ1',\n","                    'red_champ2', 'red_champ3', 'red_champ4', 'red_champ5', 'blue_top', 'red_top', 'blue_jungle', \n","                    'red_jungle', 'blue_middle', 'red_middle', 'blue_adc', 'red_adc', 'blue_support', 'red_support']\n","\n","if is_train:\n","  indices = train_indices\n","  train_string = \"Train\"\n","else:\n","  indices = test_indices\n","  train_string = \"Test\"\n"],"execution_count":0,"outputs":[]},{"metadata":{"id":"Qg3e2_Xi0xMS","colab_type":"code","colab":{"base_uri":"https://localhost:8080/","height":98},"outputId":"501a976c-54b9-4bda-9664-737b2b5a52f9","executionInfo":{"status":"ok","timestamp":1556672590900,"user_tz":240,"elapsed":6253,"user":{"displayName":"Sadat Shaik","photoUrl":"","userId":"02158574928113400599"}}},"cell_type":"code","source":["champ_names = pd.read_csv(casual_base_url + 'champs.csv')\n","\n","champ_names['name'] = champ_names['name'].str.lower()\n","champ_names['name'] = champ_names['name'].str.replace(' ', '')\n","\n","\n","data_dict=champ_names.set_index('name')['id'].to_dict()\n","\n","data_dict['zoe'] = 519\n","\n","for label in columns_to_convert:\n","  (professional[label])=professional[label].str.lower()\n","  professional[label][professional[label]=='monkeyking']='wukong'\n","  profess